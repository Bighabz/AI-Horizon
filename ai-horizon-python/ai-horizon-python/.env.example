# Gemini API Configuration (3 keys from DIFFERENT Google Cloud projects for separate quotas)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_KEY_2=your_second_api_key
GEMINI_API_KEY_3=your_third_api_key

# File Search Store Names (populated after running setup_file_stores.py)
DCWF_STORE_NAME=fileSearchStores/your-dcwf-store-id
ARTIFACTS_STORE_NAME=fileSearchStores/your-artifacts-store-id

# Optional: YouTube API (if needed for private videos)
YOUTUBE_API_KEY=

# Optional: Dumpling.ai API key (fallback web scraper)
DUMPLING_API_KEY=

# Logging
LOG_LEVEL=INFO

# Supabase (for production database)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key
